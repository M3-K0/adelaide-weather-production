{
  "dashboard": {
    "id": null,
    "title": "Adelaide Weather API - Load Testing Dashboard",
    "tags": ["load-testing", "performance", "weather-api"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "gnetId": null,
    "graphTooltip": 1,
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "name": "test_type",
          "type": "custom",
          "label": "Test Type",
          "query": "baseline,target_load,stress_test,spike_test,capacity_test,endurance_test",
          "options": [
            {"text": "Baseline", "value": "baseline"},
            {"text": "Target Load", "value": "target_load"},
            {"text": "Stress Test", "value": "stress_test"},
            {"text": "Spike Test", "value": "spike_test"},
            {"text": "Capacity Test", "value": "capacity_test"},
            {"text": "Endurance Test", "value": "endurance_test"}
          ],
          "current": {"text": "All", "value": "$__all"},
          "multi": true,
          "includeAll": true
        },
        {
          "name": "region",
          "type": "custom",
          "label": "Region",
          "query": "us-east,us-west,europe,asia-pacific,local",
          "options": [
            {"text": "US East", "value": "us-east"},
            {"text": "US West", "value": "us-west"},
            {"text": "Europe", "value": "europe"},
            {"text": "Asia Pacific", "value": "asia-pacific"},
            {"text": "Local", "value": "local"}
          ],
          "current": {"text": "All", "value": "$__all"},
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Load Test Events",
          "datasource": "Prometheus",
          "enable": true,
          "expr": "changes(weather_api_load_test_start_time[1h])",
          "iconColor": "blue",
          "titleFormat": "Load Test Started",
          "textFormat": "Test Type: {{test_type}}"
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "title": "Load Test Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "weather_api_load_test_active_users",
            "legendFormat": "Active Users",
            "refId": "A"
          },
          {
            "expr": "rate(weather_api_load_test_requests_total[5m])*60",
            "legendFormat": "Requests/min",
            "refId": "B"
          },
          {
            "expr": "weather_api_load_test_error_rate * 100",
            "legendFormat": "Error Rate %",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "displayMode": "basic",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 95}
              ]
            }
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 2,
        "title": "Response Time Trends",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(weather_api_load_test_response_time_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.90, rate(weather_api_load_test_response_time_bucket[5m]))",
            "legendFormat": "P90",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(weather_api_load_test_response_time_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.99, rate(weather_api_load_test_response_time_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "D"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            },
            "unit": "ms",
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 2000}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull", "max"]}
        }
      },
      {
        "id": 3,
        "title": "Throughput & Error Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(weather_api_load_test_requests_total[5m])*60",
            "legendFormat": "Requests/min",
            "refId": "A"
          },
          {
            "expr": "rate(weather_api_load_test_errors_total[5m])*60",
            "legendFormat": "Errors/min",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            },
            "unit": "reqps",
            "min": 0
          }
        },
        "options": {
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull", "max"]}
        }
      },
      {
        "id": 4,
        "title": "System Resource Usage",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total[5m]) * 100",
            "legendFormat": "CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "process_resident_memory_bytes / (1024*1024)",
            "legendFormat": "Memory Usage MB",
            "refId": "B"
          },
          {
            "expr": "rate(process_open_fds[5m])",
            "legendFormat": "Open File Descriptors",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "line"}
            },
            "unit": "short",
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull", "max"]}
        }
      },
      {
        "id": 5,
        "title": "API Endpoint Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(forecast_requests_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "/forecast P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(health_requests_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "/health P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(metrics_requests_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "/metrics P95",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "line"}
            },
            "unit": "ms",
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 2000}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull", "max"]}
        }
      },
      {
        "id": 6,
        "title": "Regional Performance Comparison",
        "type": "bargauge",
        "targets": [
          {
            "expr": "avg by (region) (histogram_quantile(0.95, rate(weather_api_load_test_response_time_bucket{region=~\"$region\"}[5m])))",
            "legendFormat": "{{region}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 24},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "custom": {
              "displayMode": "basic",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 2000}
              ]
            },
            "unit": "ms",
            "min": 0,
            "max": 3000
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "displayMode": "basic",
          "showUnfilled": true
        }
      },
      {
        "id": 7,
        "title": "Cache Performance During Load Test",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(weather_api_cache_hits_total[5m]) / (rate(weather_api_cache_hits_total[5m]) + rate(weather_api_cache_misses_total[5m])) * 100",
            "legendFormat": "Cache Hit Rate %",
            "refId": "A"
          },
          {
            "expr": "rate(weather_api_cache_operations_total[5m])*60",
            "legendFormat": "Cache Operations/min",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 24},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 1,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "line"}
            },
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 60},
                {"color": "green", "value": 80}
              ]
            }
          }
        },
        "options": {
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull", "max"]}
        }
      },
      {
        "id": 8,
        "title": "Error Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (status_code) (increase(weather_api_load_test_errors_total[1h]))",
            "legendFormat": "{{status_code}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 24},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "hideFrom": {"legend": false, "tooltip": false, "vis": false}
            },
            "mappings": [],
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "right", "calcs": ["lastNotNull"]}
        }
      },
      {
        "id": 9,
        "title": "Load Test Timeline",
        "type": "timeseries",
        "targets": [
          {
            "expr": "weather_api_load_test_active_users",
            "legendFormat": "Active Virtual Users",
            "refId": "A"
          },
          {
            "expr": "weather_api_load_test_target_users",
            "legendFormat": "Target Users",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 20,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "scaleDistribution": {"type": "linear"},
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            },
            "unit": "users",
            "min": 0
          }
        },
        "options": {
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "bottom", "calcs": ["lastNotNull", "max"]}
        }
      },
      {
        "id": 10,
        "title": "SLA Compliance",
        "type": "stat",
        "targets": [
          {
            "expr": "(sum(rate(weather_api_load_test_requests_total[5m])) - sum(rate(weather_api_load_test_requests_total{response_time_bucket=\"le\",le=\"2000\"}[5m]))) / sum(rate(weather_api_load_test_requests_total[5m])) * 100",
            "legendFormat": "P95 < 2s Compliance %",
            "refId": "A"
          },
          {
            "expr": "100 - (sum(rate(weather_api_load_test_errors_total[5m])) / sum(rate(weather_api_load_test_requests_total[5m])) * 100)",
            "legendFormat": "Error Rate < 1% Compliance %",
            "refId": "B"
          },
          {
            "expr": "(sum(rate(weather_api_load_test_requests_total[5m])) > 10) * 100",
            "legendFormat": "Throughput > 10 req/s Compliance %",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "custom": {
              "displayMode": "basic",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "horizontal",
          "textMode": "auto",
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center"
        }
      },
      {
        "id": 11,
        "title": "Capacity Analysis",
        "type": "table",
        "targets": [
          {
            "expr": "label_replace(weather_api_load_test_active_users, \"metric\", \"Active Users\", \"__name__\", \".*\")",
            "format": "table",
            "refId": "A"
          },
          {
            "expr": "label_replace(rate(weather_api_load_test_requests_total[5m])*60, \"metric\", \"Requests/min\", \"__name__\", \".*\")",
            "format": "table",
            "refId": "B"
          },
          {
            "expr": "label_replace(histogram_quantile(0.95, rate(weather_api_load_test_response_time_bucket[5m])), \"metric\", \"P95 Response Time (ms)\", \"__name__\", \".*\")",
            "format": "table",
            "refId": "C"
          },
          {
            "expr": "label_replace(rate(weather_api_load_test_errors_total[5m])/rate(weather_api_load_test_requests_total[5m])*100, \"metric\", \"Error Rate %\", \"__name__\", \".*\")",
            "format": "table",
            "refId": "D"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 40},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "custom": {
              "displayMode": "color-background",
              "inspect": false
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "red", "value": 95}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "showHeader": true,
          "sortBy": [{"desc": false, "displayName": "metric"}]
        },
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true},
              "indexByName": {"metric": 0, "Value": 1},
              "renameByName": {"metric": "Metric", "Value": "Current Value"}
            }
          }
        ]
      }
    ],
    "refresh": "5s",
    "schemaVersion": 30,
    "version": 1,
    "links": [
      {
        "title": "Load Testing Documentation",
        "url": "https://github.com/your-org/weather-forecast/tree/main/tests/load",
        "type": "link",
        "icon": "doc"
      },
      {
        "title": "API Documentation",
        "url": "http://localhost:8000/docs",
        "type": "link",
        "icon": "cloud"
      }
    ]
  }
}