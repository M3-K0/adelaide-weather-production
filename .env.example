# Adelaide Weather Forecasting System - Environment Configuration
# Copy this file to .env and set your actual values

# REQUIRED: API Authentication Token (NEVER commit real tokens to git)
API_TOKEN=your-secure-api-token-here

# Optional: API base URL (defaults to http://localhost:8000 in development)
API_BASE_URL=http://localhost:8000

# Optional: Environment (development, staging, production)
ENVIRONMENT=development

# Optional: CORS origins (comma-separated)
CORS_ORIGINS=http://localhost:3000

# Optional: Logging level
LOG_LEVEL=INFO

# Optional: Rate limiting
RATE_LIMIT_PER_MINUTE=60