# Adelaide Weather Forecasting - Staging Environment
# Copy this file to .env for staging deployment

# Environment Configuration
ENVIRONMENT=staging
NODE_ENV=staging

# API Configuration (Change these values!)
API_TOKEN=staging-token-replace-with-secure-value
CORS_ORIGINS=http://localhost:3000
LOG_LEVEL=INFO
RATE_LIMIT_PER_MINUTE=100

# Performance Settings
PERFORMANCE_CACHE_TTL=180

# Monitoring
GRAFANA_PASSWORD=staging-password-change-me

# Redis Configuration
REDIS_URL=redis://redis:6379
REDIS_MAX_MEMORY=128mb

# Quality Assurance
ENABLE_DATA_VALIDATION=true
VALIDATION_THRESHOLD=0.95

# Staging-specific flags
ENABLE_MONITORING=true
METRICS_INTERVAL=60